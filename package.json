{
    "name": "mtx-syntax-color",
    "displayName": "mtx-syntax-color",
    "description": "set-description-later",
    "publisher": "MarcoKoenig",
    "author": {
        "name": "Marco KÃ¶nig"
    },
    "keywords": ["Bosch Rexroth", "MTX", "mtx", "Bosch", "MXT Syntax"],
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": {
        "url": "https://github.com/tk-koenig/vscode_mtx_lang_ext.git"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "mtx syntax color",
                "uiTheme": "vs-dark",
                "path": "./themes/mtx syntax color -color-theme.json"
            }
        ],
        "languages": [
            {
                "id": "mtx_1",
                "aliases": [
                    "mtx",
                    "mtx_1"
                ],
                "extensions": [
                    ".npg"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mtx_1",
                "scopeName": "source.mtx",
                "path": "./syntaxes/mtx_1.tmLanguage.json"
            }
        ],
        "snippets": [
			{
				"language": "mtx_1",
				"path": "./snippets/mtx_1.json"
            }
        ]
    }
}